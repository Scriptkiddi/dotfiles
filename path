PATH=$HOME/bin # Binaries im home
PATH=$PATH:/usr/local/bin
PATH=$PATH:/usr/local/sbin
PATH=$PATH:/bin # System
PATH=$PATH:/sbin # System
PATH=$PATH:/usr/sbin
PATH=$PATH:/usr/bin
PATH=$PATH:/usr/X11/bin # X11
PATH=$PATH:/usr/games/bin # Games
PATH=$PATH:/usr/bin/core_perl # Perl
PATH=$PATH:/opt/java/bin # Java

export PATH

